{
  "name": "autocloud-code-challenge",
  "version": "1.0.0",
  "description": "Autocloud Breweries Code Challenge ",
  "main": "dist",
  "scripts": {
    "build": "tsc --project ./tsconfig.json",
    "watch": "tsc --project ./tsconfig.json --watch",
    "dev": "nodemon dist",
    "start": "npm run build && node dist",
    "test": "jest --verbose",
    "coverage": "jest --coverage",
    "lint": "tslint -c tslint.json 'src/**/**.ts'"
  },
  "keywords": [
    "autocloud",
    "code",
    "challenge"
  ],
  "author": "Jesus Ibanez Bondzio",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/memory-cache": "^0.2.1",
    "@types/node": "^16.9.1",
    "@types/passport": "^1.0.7",
    "@types/passport-http": "^0.3.9",
    "jest": "^27.2.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "memory-cache": "^0.2.0",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0"
  }
}
